def avg():
    tot = 0
    cnt = 0

    with open('sample.txt', 'r') as f:
        while True:
            line = f.readline()
            if not line:
                break
            score = int(line.strip())
            tot += score
            cnt += 1

    return tot / cnt

res = avg()

with open('result.txt', 'w') as f:
    f.write(str(res))

