#함수 정의
# def 함수면 (매개변수):
#   함수 정의 코드

def name(word) :
    print(word)
a='python'
name(a)

def name2(word) :
    n='HI' +(word)
    return n

x=name2('good') # 함수의 리턴값이 x 에 대입된다.
print(x)
##############################################

def multi(x) :
    y1=x*5
    y2=x*10
    y3=x*20
    return y1,y2,y3 #쉼표, 를 이용해 한번에 값을 반환 받을 수 있음

a,b,c = multi(10) #a,b,c = y1, y2, y3 언패킹
print(a,b,c) #50 100 200

##############################################

def func1(x) :
    y1=x*5
    y2=x*10
    y3=x*20
    return (y1,y2,y3)

a=func1 (5) #a 의 값은 return 값
print(a, type(a)) # (25, 50, 100) <class 'tuple'>
print(list(a)) # [25, 50, 100] a 를 list로 변환하여 출력
print(set(a)) # {25, 50, 100}


def func2(x) :
    y1=x*5
    y2=x*10
    y3=x*20
    return [y1,y2,y3] # list 로 return받기

a=func2(5)
print(a,type(a),set(a))

################################################

def func3(x) :
    y1=x*5
    y2=x*10
    y3=x*20
    return {'y1': y1,'y2':y2,'y3':y3} #dic 딕셔너리

a=func3(5)
print(a,type(a))

#y2 의 대응값 출력]
print(a['y2']) #50
# 키 밸류 다 출력
print(a.items()) #dict_items([('y1', 25), ('y2', 50), ('y3', 100)])
# 키 만 출력
print(a.keys()) #dict_keys(['y1', 'y2', 'y3'])


print('---------------------------------------------------')





def qq(a,b) : #for , if 문을 사용할 때엔 들여쓰기 주의
    for i in range(a,b) :
        print(i,end=' ')
    print()
qq(1,10) # 1~9 출력
qq(4,6) # 4~5출력 



def final(x,y,func1): #함수를 다른 함수의 매개변수로 넘길 수 있음 -> 고차함수-> mul이 final의 매개변수로
    print(x,y,func1(5,6))

final(1,2,mul)
